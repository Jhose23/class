Lista de numeros para media
import java.util.Scanner;

public class EstadisticasNumeros {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        double[] numeros = new double[10];
        double suma = 0;

        int mayores = 0;
        int iguales = 0;
        int menores = 0;

        
        for (int i = 0; i < 10; i++) {
            System.out.print("Ingrese el número " + (i + 1) + ": ");
            numeros[i] = sc.nextDouble();
            suma += numeros[i];

            if (numeros[i] > 10.5) {
                mayores++;
            } else if (numeros[i] == 10.5) {
                iguales++;
            } else {
                menores++;
            }
        }

        
        double media = suma / 10;

        // Mostrar resultados
        System.out.println("\nMedia: " + media);
        System.out.println("Mayores que 10.5: " + mayores);
        System.out.println("Iguales a 10.5: " + iguales);
        System.out.println("Menores que 10.5: " + menores);

        sc.close();
    }
}

